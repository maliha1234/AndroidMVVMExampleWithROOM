<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myapplicationlogintest">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.example.demoLogin.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApplicationlogintest">
        <activity android:name="com.example.demoLogin.ui.login.CoruotinetestActivity"><intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter></activity>
        <activity
            android:name="com.example.demoLogin.ui.login.LoginNew"
            android:label="@string/title_activity_login_new"
            android:theme="@style/Theme.MyApplicationlogintest.NoActionBar">

        </activity>
        <activity
            android:name="com.example.demoLogin.ui.login.LoginActivity"
            android:label="@string/app_name"></activity>
    </application>

</manifest>